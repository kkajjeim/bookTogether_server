{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "quotemark": false,
    "ordered-imports": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "variable-name": false,
    "typedef": [true, "call-signature", "property-declaration"]
  },
  "rulesDirectory": []
}
